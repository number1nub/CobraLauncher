<?xml version="1.0"?>
<buttons>
	<default id="Default" BackColor="Yellow" TextColor="Black" HlBackColor="Black" HLTextColor="Yellow" ignore="1"></default>
	<Gui id="c0bra Main GUI">
		<button id="Programs" main="1">
			<button id="Internet Explorer" command="iexplore.exe" type="P"></button>
		</button>
		<button id="Folders" main="1">
			<button id="C Drive" command="C:\" type="R"></button>
			<button id="Program Files" command="%ProgramFiles%" type="P"></button>
			<button id="My Documents" command="%A_MyDocuments%" type="P"></button>
		</button>
		<button id="Bookmarks" main="1"></button>
		<button id="Utilities" main="1">
			<button id="Notepad" command="notepad.exe" type="P"></button>
			<button id="Screen Clipper" command="%windir%\System32\SnippingTool.exe" type="P"></button>
		</button>
	</Gui>
	<Gui id="SLRGui">
		<default id="Default" BackColor="Red" TextColor="White" HlBackColor="Yellow" HlTextColor="Black" ignore="1"></default>
		<button id="Cancel"></button>
		<button id="Logoff"></button>
		<button id="Restart"></button>
		<button id="Shutdown"></button>		
	</Gui>
</buttons>
